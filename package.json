{
	"name": "highlight-bot",
	"version": "3.0.0",
	"description": "An utility bot written in Sapphire to get highlighted when a word, or regular expression is said",
	"main": "dist/Highlight.js",
	"type": "module",
	"scripts": {
		"build:ci": "tsc --noEmit",
		"build": "tsc",
		"clean": "npx rimraf dist",
		"cleanbuild": "npm run clean && tsc",
		"coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
		"lint": "eslint --fix src --ext ts",
		"start": "npm run cleanbuild && node .",
		"test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
		"test:lint": "eslint src --ext mjs,js,ts",
		"watch": "tsc -w",
		"postinstall": "node scripts/postinstall-packagejson-fix.mjs"
	},
	"private": true,
	"author": {
		"name": "Vlad Frangu",
		"email": "kingdgrizzle@gmail.com"
	},
	"engines": {
		"node": ">= 16.0.0"
	},
	"dependencies": {
		"@discordjs/builders": "*",
		"@mikro-orm/sql-highlighter": "^1.0.1",
		"@prisma/client": "^3.10.0",
		"@sapphire/decorators": "^4.3.0",
		"@sapphire/discord.js-utilities": "^4.9.0",
		"@sapphire/framework": "next",
		"@sapphire/plugin-logger": "^2.2.0",
		"@sapphire/stopwatch": "^1.4.0",
		"@sapphire/time-utilities": "^1.7.0",
		"@sapphire/type": "^2.2.0",
		"@sapphire/utilities": "^3.4.0",
		"bufferutil": "^4.0.6",
		"colorette": "^2.0.16",
		"confusables": "^1.1.0",
		"discord-api-types": "*",
		"discord.js": "^13.6.0",
		"node-fetch": "^3.2.1",
		"re2": "^1.17.4",
		"tslib": "^2.3.1",
		"utf-8-validate": "^5.0.8",
		"zlib-sync": "^0.1.7"
	},
	"devDependencies": {
		"@sapphire/eslint-config": "^4.3.0",
		"@sapphire/ts-config": "^3.3.2",
		"@types/jest": "^27.4.1",
		"@types/node": "^17.0.21",
		"@types/node-fetch": "^2.6.1",
		"@typescript-eslint/eslint-plugin": "^5.13.0",
		"@typescript-eslint/parser": "^5.13.0",
		"eslint": "^8.10.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"jest": "^27.5.1",
		"jest-circus": "^27.5.1",
		"prettier": "^2.5.1",
		"prisma": "^3.10.0",
		"ts-jest": "^27.1.3",
		"ts-node": "^10.7.0",
		"typescript": "^4.7.0-dev.20220304"
	},
	"imports": {
		"#internals/*": "./dist/lib/internals/*.js",
		"#hooks/*": "./dist/lib/utils/hooks/*.js",
		"#setup": "./dist/lib/setup.js",
		"#structures/*": "./dist/lib/structures/*.js",
		"#types/*": "./dist/lib/types/*.js",
		"#utils/*": "./dist/lib/utils/*.js",
		"#workers/*": "./dist/lib/workers/*.js"
	}
}
